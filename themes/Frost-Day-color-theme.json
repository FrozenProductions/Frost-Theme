    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#8790AB",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other.object",
                "variable.language"
            ],
            "settings": {
                "foreground": "#2A2E3C"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#6B8AFF"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.template",
                "string.quoted.single",
                "string.quoted.double"
            ],
            "settings": {
                "foreground": "#7AC74F"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FF6B88"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#4C6FE6"
            }
        },
        {
            "name": "Classes and Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#E5B45C"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#6B8AFF"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#B87BFF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#4CCCF9"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "meta.decorator variable.other.readwrite",
                "meta.decorator entity.name.function"
            ],
            "settings": {
                "foreground": "#E5B45C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass"
            ],
            "settings": {
                "foreground": "#4CCCF9"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#7AC74F"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF6B88"
            }
        },
        {
            "name": "Terminal Colors",
            "scope": ["terminal.ansiBlack"],
            "settings": {
                "foreground": "#2A2E3C"
            }
        },
        {
            "name": "Terminal Bright Colors",
            "scope": ["terminal.ansiBrightBlack"],
            "settings": {
                "foreground": "#4A5474"
            }
        },
        {
            "name": "JSX/TSX Tags",
            "scope": [
                "punctuation.definition.tag.jsx",
                "punctuation.definition.tag.tsx"
            ],
            "settings": {
                "foreground": "#6B8AFF"
            }
        },
        {
            "name": "JSX/TSX Components",
            "scope": [
                "support.class.component.jsx",
                "support.class.component.tsx"
            ],
            "settings": {
                "foreground": "#E5B45C"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#6B8AFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#4CCCF9",
                "fontStyle": "underline"
            }
        }
    ]
} 