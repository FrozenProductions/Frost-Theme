    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#6A7AA3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other.object",
                "variable.language"
            ],
            "settings": {
                "foreground": "#D0DDEE"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#88A4FF"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.template",
                "string.quoted.single",
                "string.quoted.double"
            ],
            "settings": {
                "foreground": "#B4FFD0"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FFA3A9"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#ADD6FF"
            }
        },
        {
            "name": "Classes and Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#FFE08F"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#88A4FF"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#C9B3FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#95F5FF"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "meta.decorator variable.other.readwrite",
                "meta.decorator entity.name.function"
            ],
            "settings": {
                "foreground": "#FFE08F",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass"
            ],
            "settings": {
                "foreground": "#95F5FF"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#B4FFD0"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF6B88"
            }
        },
        {
            "name": "Terminal Colors",
            "scope": ["terminal.ansiBlack"],
            "settings": {
                "foreground": "#1A1B26"
            }
        },
        {
            "name": "Terminal Bright Colors",
            "scope": ["terminal.ansiBrightBlack"],
            "settings": {
                "foreground": "#565A73"
            }
        },
        {
            "name": "JSX/TSX Tags",
            "scope": [
                "punctuation.definition.tag.jsx",
                "punctuation.definition.tag.tsx"
            ],
            "settings": {
                "foreground": "#88A4FF"
            }
        },
        {
            "name": "JSX/TSX Components",
            "scope": [
                "support.class.component.jsx",
                "support.class.component.tsx"
            ],
            "settings": {
                "foreground": "#FFE08F"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#88A4FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#95F5FF",
                "fontStyle": "underline"
            }
        }
    ]
} 