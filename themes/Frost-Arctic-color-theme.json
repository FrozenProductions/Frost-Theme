	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#506685",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables and Parameters",
			"scope": [
				"variable",
				"variable.parameter",
				"variable.other.object",
				"variable.language"
			],
			"settings": {
				"foreground": "#B7C5D3"
			}
		},
		{
			"name": "Keywords and Control",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#7289DA"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.template",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#A3E8B9"
			}
		},
		{
			"name": "Numbers and Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#FF8B92"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#99BDFF"
			}
		},
		{
			"name": "Classes and Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#FFD280"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#7289DA"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#B19CD9",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"meta.decorator",
				"meta.decorator variable.other.readwrite",
				"meta.decorator entity.name.function"
			],
			"settings": {
				"foreground": "#FFD280",
				"fontStyle": "italic"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss",
				"support.type.property-name.less",
				"support.type.property-name.sass"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#A3E8B9"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Terminal Colors",
			"scope": ["terminal.ansiBlack"],
			"settings": {
				"foreground": "#181821"
			}
		},
		{
			"name": "Terminal Bright Colors",
			"scope": ["terminal.ansiBrightBlack"],
			"settings": {
				"foreground": "#4A4A5E"
			}
		},
		{
			"name": "JSX/TSX Tags",
			"scope": [
				"punctuation.definition.tag.jsx",
				"punctuation.definition.tag.tsx"
			],
			"settings": {
				"foreground": "#7289DA"
			}
		},
		{
			"name": "JSX/TSX Components",
			"scope": [
				"support.class.component.jsx",
				"support.class.component.tsx"
			],
			"settings": {
				"foreground": "#FFD280"
			}
		},
		{
			"name": "Markdown Headers",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#7289DA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.underline.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#89DDFF",
				"fontStyle": "underline"
			}
		}
	]
}