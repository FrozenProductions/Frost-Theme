    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#8490C8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables and Parameters",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other.object",
                "variable.language"
            ],
            "settings": {
                "foreground": "#E9F0FF"
            }
        },
        {
            "name": "Keywords and Control",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#A4BDFF"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.template",
                "string.quoted.single",
                "string.quoted.double"
            ],
            "settings": {
                "foreground": "#C5FFE8"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FFBBC0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#C1EAFF"
            }
        },
        {
            "name": "Classes and Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#FFE8B3"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#A4BDFF"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#DCC9FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#B3FFFF"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "meta.decorator variable.other.readwrite",
                "meta.decorator entity.name.function"
            ],
            "settings": {
                "foreground": "#FFE8B3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass"
            ],
            "settings": {
                "foreground": "#B3FFFF"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#C5FFE8"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF8FA6"
            }
        },
        {
            "name": "Terminal Colors",
            "scope": ["terminal.ansiBlack"],
            "settings": {
                "foreground": "#1C1D2A"
            }
        },
        {
            "name": "Terminal Bright Colors",
            "scope": ["terminal.ansiBrightBlack"],
            "settings": {
                "foreground": "#666B8C"
            }
        },
        {
            "name": "JSX/TSX Tags",
            "scope": [
                "punctuation.definition.tag.jsx",
                "punctuation.definition.tag.tsx"
            ],
            "settings": {
                "foreground": "#A4BDFF"
            }
        },
        {
            "name": "JSX/TSX Components",
            "scope": [
                "support.class.component.jsx",
                "support.class.component.tsx"
            ],
            "settings": {
                "foreground": "#FFE8B3"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#A4BDFF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#B3FFFF",
                "fontStyle": "underline"
            }
        }
    ]
}