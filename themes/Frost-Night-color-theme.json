	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#405675",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables and Parameters",
			"scope": [
				"variable",
				"variable.parameter",
				"variable.other.object",
				"variable.language"
			],
			"settings": {
				"foreground": "#A7B5C3"
			}
		},
		{
			"name": "Keywords and Control",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#5B73C4"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.template",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#93D8A9"
			}
		},
		{
			"name": "Numbers and Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#EF7B82"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#89ADEF"
			}
		},
		{
			"name": "Classes and Types",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#EFC270"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#5B73C4"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#A18CC9",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#79CDFF"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"meta.decorator",
				"meta.decorator variable.other.readwrite",
				"meta.decorator entity.name.function"
			],
			"settings": {
				"foreground": "#EFC270",
				"fontStyle": "italic"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss",
				"support.type.property-name.less",
				"support.type.property-name.sass"
			],
			"settings": {
				"foreground": "#79CDFF"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#93D8A9"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#EF4360"
			}
		},
		{
			"name": "Terminal Colors",
			"scope": ["terminal.ansiBlack"],
			"settings": {
				"foreground": "#0F0F14"
			}
		},
		{
			"name": "Terminal Bright Colors",
			"scope": ["terminal.ansiBrightBlack"],
			"settings": {
				"foreground": "#3A3A4E"
			}
		},
		{
			"name": "JSX/TSX Tags",
			"scope": [
				"punctuation.definition.tag.jsx",
				"punctuation.definition.tag.tsx"
			],
			"settings": {
				"foreground": "#5B73C4"
			}
		},
		{
			"name": "JSX/TSX Components",
			"scope": [
				"support.class.component.jsx",
				"support.class.component.tsx"
			],
			"settings": {
				"foreground": "#EFC270"
			}
		},
		{
			"name": "Markdown Headers",
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#5B73C4",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"markup.underline.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#79CDFF",
				"fontStyle": "underline"
			}
		}
	]
} 